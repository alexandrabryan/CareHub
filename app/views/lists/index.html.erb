
<h1>Lists</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <tr>
    <% @lists.each do |list| %>

        <td> <%= link_to list.title, list_path(list) %> </td>
        </tr>
        <% end %>
  </tbody>
</table>

<br>

